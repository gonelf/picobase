{
  "name": "@picobase_app/client",
  "version": "0.1.0",
  "description": "PicoBase client SDK â€” auth, database, storage, and realtime for your apps",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "pocketbase": "^0.25.2"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@vitest/ui": "^2.1.8",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "picobase",
    "pocketbase",
    "baas",
    "backend",
    "auth",
    "database",
    "realtime"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gonelf/picobase",
    "directory": "packages/client"
  }
}
